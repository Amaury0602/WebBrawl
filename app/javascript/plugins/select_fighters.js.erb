export function selectFighters() {
  const selectButtons = document.querySelectorAll('.select-fighter');
  selectButtons.forEach((button) => {
    button.addEventListener('click', (e) => {
      e.currentTarget.classList.toggle('active');
      const activeFighters = document.getElementsByClassName('active')
      if (activeFighters[0]) {
        console.log(activeFighters[0].attributes[1].value) // l'id des fighters selectionn√©s
      }
    })
  })
}
